{
	"name": "@registry/kit",
	"private": true,
	"version": "0.0.2",
	"type": "module",
	"packageManager": "pnpm@10.17.1",
	"scripts": {
		"dev": "concurrently -n \"registry,app\" -c \"blue,green\" \"jsrepo build --watch\" \"vite dev\"",
		"build": "pnpm registry:build && vite build",
		"preview": "pnpm registry:build && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"registry:build": "jsrepo build",
		"registry:deploy": "jsrepo build --watch",
		"ci:release": "jsrepo publish && changeset tag"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@shikijs/langs": "^3.12.2",
		"@shikijs/themes": "^3.12.2",
		"@sveltejs/adapter-vercel": "^6.0.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^22",
		"bits-ui": "^2.14.3",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"isomorphic-dompurify": "2.25.0",
		"jsrepo": "3.0.0-beta.19",
		"mode-watcher": "^1.1.0",
		"package-manager-detector": "^1.5.0",
		"paneforge": "^1.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"runed": "^0.31.1",
		"shiki": "^3.15.0",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"svelte-meta-tags": "^4.5.0",
		"svelte-toolbelt": "^0.9.3",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.1.10",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"pnpm": "^10.22.0"
	}
}
